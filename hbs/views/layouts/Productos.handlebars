<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css" type="text/css">

</head>
<body>
    
    {{>header}}

    <main>

        <div class="columnas">
            <div>ID</div>
            <div>Titulo</div>
            <div>Precio</div>
        </div>
        {{#each productos}}

            <div class="producto">
                <p>{{this.id}}</p>
                <p>{{this.title}}</p>
                <p>{{this.precio}}</p>
            </div>
        {{/each }}
    </main>
    <script src="index.js"></script>

</body>
</html>